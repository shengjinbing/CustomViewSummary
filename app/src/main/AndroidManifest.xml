<?xml version="1.0" encoding="utf-8"?>
<manifest package="cn.lixiang.customviewsummary"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.CakeActivity">
        </activity>
        <activity android:name=".activity.PieGraphActivity">
        </activity>
        <activity android:name=".activity.SlideSwitchActivity">
        </activity>
        <activity android:name=".activity.TestActivity">
        </activity>
        <activity android:name=".activity.TrendChartActivity">
        </activity>
    </application>

</manifest>